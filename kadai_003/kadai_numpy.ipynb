      "source": [
        "import numpy as np\n",
        "\n",
        "A = np.arange(6).reshape(3, 2)\n",
        "print(\"A行列 = \")\n",
        "print(A)\n",
        "print()\n",
        "B = np.arange(8).reshape(2, 4)\n",
        "print(\"B行列 = \")\n",
        "print(B)\n",
        "print()\n",
        "dot_AB = np.dot(A, B)\n",
        "print(\"A･Bの演算結果:\")\n",
        "print(dot_AB)\n",
        "print()\n",
        "dot_AB_max = dot_AB.max()\n",
        "print(\"A･Bの演算結果要素の最大値:\")\n",
        "print(dot_AB_max)"
      ]
